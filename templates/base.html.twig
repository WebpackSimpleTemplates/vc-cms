<!DOCTYPE html>
<html class="{% block html_class %}bg-base-200{% endblock %}">
    <head>
        <link rel="preload" href="/bg.jpg" as="image" >
        <link rel="icon" href="/favicon.ico">
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% block stylesheets %}
            {{ vite_entry_link_tags('appcss') }}
        {% endblock %}

        {% block javascripts %}
            {{ vite_entry_script_tags('appjs') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
